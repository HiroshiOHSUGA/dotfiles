[user]
	name = umezo
	email = umezo@tsuyabu.in

[alias]
  graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
  merge = merge --no-ff 
  br = branch
  st = status
  pr = pull --rebase --autostash
  push = push -u
  branch-info = for-each-ref refs/remotes --sort=committerdate --format=\"%(committerdate:short) %(committername) %(refname:short)\"

[color]
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto


[core]
  excludesfile = ~/.gitignore.personal
